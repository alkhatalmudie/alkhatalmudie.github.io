rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /orders/{orderId} {
      // Public can create, but only YOU (Admin) can read
      allow create: if true;
      allow read, delete: if request.auth != null && request.auth.uid == 'EOQEt04azKTTWcmNXVtpgSVsRxt2';
    }
  }
}