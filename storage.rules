rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /client_uploads/{allPaths=**} {
      // Anyone can upload their passport/docs
      allow write: if true;
      // ONLY the Admin can see or download them
      allow read: if request.auth != null && request.auth.uid == 'EOQEt04azKTTWcmNXVtpgSVsRxt2';
    }
  }
}